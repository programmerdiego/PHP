<html>
<?php
$myArray=array("mr","diego","sanchez");
array_unshift($myArray,"START");
array_push($myArray, "END");
echo("$myArray[0]");
echo("$myArray[1]");
echo("$myArray[2]");
echo("$myArray[3]");
echo("$myArray[4]");
?>
</html>

/* 2: Sample run
