/* 1: Main Method
-------------------------------------------------------------- */
<html>
<?php
$myArray=array("1","2","3");
$myArray2=array("4","5");
$arr=array_merge($myArray,$myArray2);
foreach($arr as $value){
	echo("$value");
}
?>
</html>

